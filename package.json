{
  "name": "Multiple Heritage-cardigans",
  "version": "1.0.0",
  "description": "E-commerce platform for Naira Cardigans",
  "main": "server/index.js",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "server": "nodemon server/index.js",
    "client": "npm run dev --prefix client",
    "build": "npm run build --prefix client",
    "start": "node server/index.js",
    "seed": "node server/seeders/sampleData.js",
    "test": "cross-env NODE_ENV=test JWT_SECRET=test-secret jest --config server/jest.config.js --runInBand",
    "test:watch": "cross-env NODE_ENV=test JWT_SECRET=test-secret jest --config server/jest.config.js --runInBand --watch",
    "test:coverage": "cross-env NODE_ENV=test JWT_SECRET=test-secret jest --config server/jest.config.js --runInBand --coverage"
  },
  "keywords": [
    "ecommerce",
    "mern",
    "cardigans"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "axios": "^1.6.2",
    "bcryptjs": "^3.0.3",
    "cloudinary": "^2.8.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.3",
    "mongoose": "^8.0.3",
    "multer": "^2.0.2",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@babel/core": "^7.28.6",
    "@babel/preset-env": "^7.28.6",
    "babel-jest": "^30.2.0",
    "concurrently": "^8.2.2",
    "cross-env": "^10.1.0",
    "jest": "^30.2.0",
    "mongodb-memory-server": "^11.0.1",
    "nodemon": "^3.0.2",
    "supertest": "^7.2.2"
  }
}
